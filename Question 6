#include <stdio.h>
#include <stdlib.h>

int main()
{
    int even=0, odd=0,i, x;

    for(x=0; x<20; x++)
    {
        printf("Please Enter Values %d: ", x+1);
        scanf("%d", &i);

       if(i==0) return printf("\n 0 Not allowed!\n");

        if(i % 2 == 0) even++;
        else if(i % 2 != 0) odd++;
    }

    printf(" Even Numbers = %d \nOdd Numbers = %d\n",even, odd);

    return 0;

}
